#: timeset.sh:23
msgid "Must be root to run this script"
msgstr "Must be root to run this script"

#: timeset.sh:29
msgid "Press Enter. . ."
msgstr "Press Enter. . ."

#: timeset.sh:35
msgid " TimeSet(tings) - Configure the system date and time"
msgstr " TimeSet(tings) - Configure the system date and time"

#: timeset.sh:38
msgid "Show current date and time configuration"
msgstr "Show Current Date and Time Configuration"

#: timeset.sh:39
msgid "Show known timezones (press q to return to menu)"
msgstr "Show Known Timezones (press q to return to menu)"

#: timeset.sh:40
msgid "Set system timezone"
msgstr "Set System Timezone"

#: timeset.sh:41
msgid "Synchronize time from the network (NTP)"
msgstr "Synchronize Time from the Network (NTP)"

#: timeset.sh:42
msgid "Choose whether NTP is enabled or not"
msgstr "Control whether NTP is used or not"

#: timeset.sh:43
msgid "Control whether hardware clock is in local time or not"
msgstr "Control whether Hardware Clock is in Local Time or not"

#: timeset.sh:44
msgid "Read the time from the hardware clock"
msgstr "Read the time from the Hardware Clock"

#: timeset.sh:45
msgid "Synchronize hardware clock to system time"
msgstr "Synchronize Hardware Clock to System Time"

#: timeset.sh:46
msgid "Synchronize system time to hardware clock"
msgstr "Synchronize System Time from Hardware Clock"

#: timeset.sh:47
msgid "Set system time manually"
msgstr "Set System Time manually"

#: timeset.sh:49
msgid "Exit/Quit"
msgstr "Exit/Quit"

#: timeset.sh:51
msgid "Enter your choice"
msgstr "Enter your choice"
